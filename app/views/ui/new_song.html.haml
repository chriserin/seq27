%song
  %grids
    %pitchGrid
      - (0..127).to_a.reverse.each do |pitch|
        %pitch{class: [sharpForNumber(pitch), cPitch(pitch), octave(pitch)].join(' ')}
          %pianoKey{class: [sharpForNumber(pitch), cPitch(pitch), octave(pitch)].join(' ')}
            = cOctave(pitch)
    %beatGrid{data: {stage: 'beats', scene: 'sixteen'}}
      - 16.times.each do
        %beat
    %beatGrid{data: {stage: 'beats', scene: 'overflow'}}
      - 32.times.each do
        %beat
    %cursorGrid{data: {stage: 'selection', scene: 'none'}}
      %cursor{style: "top: #{16 * 60}px; left: 60px;"}
    %cursorGrid{data: {stage: 'selection', scene: 'selected'}}
      %cursor{style: "top: #{16 * 60}px; left: 260px;"}
      %anchorCursor{style: "top: #{16 * 79}px; left: 560px;"}
      %visualSelection{style: "top: #{16 * 60}px; left: 260px; height: #{16 * 20}px; width: 400px"}
    %part
      %note{style: "top: #{16 * (127 - 60)}px; left: 160px; width: 98px"}
      %note{style: "top: #{16 * (127 - 57)}px; left: 160px; width: 200px"}
      %note{style: "top: #{16 * (127 - 53)}px; left: 160px; width: 98px"}
      %note{style: "top: #{16 * (127 - 49)}px; left: 160px; width: 40px"}
  %statusLine
    %name seq27
    %filler
    %cursorPosition
      %span.label pitch:
      %span.value c6
      %span.label beats:
      %span.value 11/16
      %span.label ticks:
      %span.value 45
  %commandLine{data: {stage: 'commandLine', scene: 'partialSequence'}}
    %partialSequence
      g
  %commandLine{data: {stage: 'commandLine', scene: 'command'}}
    \:Explore
  %commandLine{data: {stage: 'commandLine', scene: 'list'}}
    \:outputs
    %p
      %span.lefttab 0
      %span.lefttab AudioBoxUSB
    %p
      %span.lefttab 1
      %span.lefttab Saphirre40
    %p.action
      %span.lefttab Press ENTER to continue
  %commandLine{data: {stage: 'commandLine', scene: 'error'}}
    %span.error
      E492: Not an editor command: something
